{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Server",
	"extends": "./base.json",
	"compilerOptions": {
		"sourceMap": true,
		"noEmit": false,
		"lib": ["ES2020", "DOM"],
		"module": "es2020",
		"moduleResolution": "node",
		"target": "ES2020",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true
	}
}
